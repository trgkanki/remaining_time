{
  "name": "addon_template",
  "version": "20.5.4i8",
  "description": "template for trgkanki's addon",
  "scripts": {
    "postinstall": "echo \"Make sure to install pipx on your environment! (npx analogue for python)\"",
    "build": "npm run compile && npx trgkanki-template-cli package",
    "update:template": "npx trgkanki-template-cli update",
    "link": "npx trgkanki-template-cli link",
    "dist": "npx trgkanki-template-cli release",
    "test": "npm-run-all test:py",
    "test:py": "pipx run pytest tests",
    "prepare": "husky install"
  },
  "keywords": [
    "anki",
    "anki-addon"
  ],
  "author": "trgk",
  "license": "AGPL3",
  "devDependencies": {
    "git-branch-is": "^4.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5"
  }
}